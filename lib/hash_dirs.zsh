# Named directories/shortcuts (~l => /var/log).
#
# These are in a separate file to be used from the
# ~/.dotfiles/usr/bin/shorten_path script.
#
# Local ones are in ~/.dotfiles/oh-my-zsh/lib/hash_dirs.local.zsh.
hash -d l=/var/log
hash -d d=~/Downloads
hash -d df=~/.dotfiles
hash -d omz=~/.dotfiles/oh-my-zsh
hash -d py=~/.pyenv/versions
